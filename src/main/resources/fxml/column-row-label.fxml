<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.text.Font?>

<fx:root maxHeight="18.0" prefHeight="18.0" stylesheets="@../styles/styles.css" type="javafx.scene.layout.HBox" xmlns="http://javafx.com/javafx/8.0.121" xmlns:fx="http://javafx.com/fxml/1">
   <children>
      <HBox alignment="CENTER" fillHeight="false" maxHeight="16.0" prefHeight="16.0" styleClass="colRowContainer">
         <children>
            <Label fx:id="txtObjectName" text="Untitled 1/Uptime">
               <font>
                  <Font size="10.0" />
               </font>
               <HBox.margin>
                  <Insets left="4.0" />
               </HBox.margin></Label>
            <Button fx:id="btnRemove" maxHeight="16.0" maxWidth="16.0" mnemonicParsing="false" styleClass="colRowContainerClose" text="X">
               <font>
                  <Font size="8.0" />
               </font>
            </Button>
         </children>
      </HBox>
   </children>
   <padding>
      <Insets bottom="1.0" left="1.0" right="1.0" top="1.0" />
   </padding>
</fx:root>
