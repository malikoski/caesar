<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.SplitPane?>
<?import javafx.scene.control.TabPane?>
<?import javafx.scene.control.ToolBar?>
<?import javafx.scene.control.TreeView?>
<?import javafx.scene.layout.VBox?>

<fx:root prefHeight="600.0" prefWidth="800.0" type="javafx.scene.layout.VBox" xmlns="http://javafx.com/javafx/8.0.121" xmlns:fx="http://javafx.com/fxml/1">
   <children>
      <ToolBar prefHeight="40.0" prefWidth="200.0">
        <items>
          <Button mnemonicParsing="false" text="Add Chart" />
        </items>
      </ToolBar>
      <SplitPane dividerPositions="0.4" prefHeight="160.0" prefWidth="200.0" VBox.vgrow="ALWAYS">
        <items>
            <TreeView fx:id="mBeans" prefHeight="200.0" prefWidth="200.0" />
            <TabPane fx:id="visualizations" prefHeight="200.0" prefWidth="200.0" tabClosingPolicy="UNAVAILABLE" />
        </items>
      </SplitPane>
   </children>
</fx:root>
